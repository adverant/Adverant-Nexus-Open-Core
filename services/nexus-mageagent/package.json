    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/opossum": "^8.1.9",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.5",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "axios-mock-adapter": "^2.1.0",
    "dotenv": "^17.2.4",
