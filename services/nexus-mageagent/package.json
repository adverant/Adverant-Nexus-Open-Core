{
  "name": "@mageagent/service",
  "version": "1.0.0",
  "description": "Multi-Model AI Orchestration Service with OpenRouter integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --max-old-space-size=3072 --expose-gc node_modules/.bin/ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node --max-old-space-size=3072 --expose-gc dist/index.js",
    "start:prod": "NODE_ENV=production node --max-old-space-size=3072 --expose-gc dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:chaos": "jest --selectProjects chaos",
    "test:performance": "jest --selectProjects performance",
    "test:security": "jest --selectProjects security",
    "test:functional": "ts-node tests/functional/run-functional-tests.ts",
    "test:functional:direct": "ts-node tests/functional/mageagent-api.test.ts",
    "test:functional:jest": "jest --config jest.config.functional.js",
    "test:all": "./tests/run-all-tests.sh",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage test-results-*",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "ai",
    "orchestration",
    "openrouter",
    "multi-model",
    "mageagent"
  ],
  "author": "Adverant AI",
  "license": "ISC",
  "dependencies": {
    "@adverant/database": "file:../../packages/adverant-database",
    "@adverant/errors": "file:../../packages/adverant-errors",
    "@adverant/nexus-telemetry": "file:../../packages/nexus-telemetry",
    "@adverant/resilience": "file:../../packages/adverant-resilience",
    "@google-cloud/aiplatform": "^6.1.0",
    "@google-cloud/bigquery": "^7.0.0",
    "@googlemaps/google-maps-services-js": "^3.3.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.48.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@opentelemetry/sdk-trace-base": "^2.5.1",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@qdrant/js-client-rest": "^1.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.5",
    "axios-retry": "^4.1.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.2",
    "cors": "^2.8.6",
    "epub2": "^3.0.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.9.3",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "neo4j-driver": "^5.19.0",
    "opossum": "^8.5.0",
    "p-queue": "^6.6.2",
    "pdf-parse": "^1.1.1",
    "pdf.js-extract": "^0.2.1",
    "pg": "^8.18.0",
